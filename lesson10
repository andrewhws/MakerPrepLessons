// Exercise 2

var random_number = function () {

  
  return Math.floor(Math.random() * 10);
};

var tenRandom = 10

while ( tenRandom > 0) {
  
  
  
  console.log(random_number());
  tenRandom--
  
}







// Exercise 4 factoral while function

var factoral_of_n = function (number) {
  
  var input = 1;
  while (number > 0) {
  input *= number;
    number--;
   
  } return input;
  
};

console.log(factoral_of_n(5));






// Exercise 5 FizzBuzz 2 


var countUpTo = parseInt(prompt("target number?"))

var thisThis = function (string1, string2, CountUpTo) {
  
  
  
  for (var i = 0; i < countUpTo; i++) {
  if (i % 5 === 0 && i % 3 ===0) {
console.log(string1 + string2);
  } else if (i % 5 === 0) {
    console.log(string1)
  } else if (i % 3 === 0 ) {
    console.log(string2)
  } else {console.log(i)}
  
  
  }
}
 thisThis("hey ", "you", countUpTo)
