myArray[1] = "andrewmhowes";

var cutName = function(string) {

  return string.split(" ");
   

};
cutName(myArray[0]);


var myData = {};

myData.fullName = cutName(myArray[0]);
myData.skype = myArray[1];
myData.github = "andrewhws";




